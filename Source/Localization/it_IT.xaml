    <sys:String x:Key="LOC_GI_CreateDefaultTargetFileUnexpectedError">Impossibile accedere o creare il file destinazione predefinito: {0}</sys:String>
    <sys:String x:Key="LOC_GI_FailedBackupGlosSIUnexpectedError">Backup dei file di configurazione di GlosSI non riuscito: {0}</sys:String>
    <sys:String x:Key="LOC_GI_FailedBackupSteamFileUnexpectedError">Backup del file shortcuts.vdf non riuscito: {0}</sys:String>
    <sys:String x:Key="LOC_GI_OpenLinkFailedUnexpectedError">Apertura del link non riuscita: &quot;{0}&quot;: {1}</sys:String>
