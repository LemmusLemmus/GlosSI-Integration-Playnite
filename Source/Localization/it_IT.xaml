    <!--Shortcut/Overlay Creation Verification-->
    <sys:String x:Key="LOC_GI_SteamShortcutNameNotSetError">Il nome del collegamento Steam non è stato impostato.</sys:String>
    <sys:String x:Key="LOC_GI_ShortcutTargetAlreadyExistsError">Esiste già un file di destinazione GlosSI con il nome di collegamento scelto.</sys:String>
