    <sys:String x:Key="LOC_GI_MultipleGamesSkipped"> ({0} partite sono state saltate)</sys:String>

    <!--Removing Integration from Games-->
    <sys:String x:Key="LOC_GI_NoGamesRemoved">Nessuna integrazione GlosSI/Steam è stata rimossa.</sys:String>
    <sys:String x:Key="LOC_GI_OneGameRemoved">L'integrazione GlosSI/Steam di un gioco è stata rimossa.</sys:String>
    <sys:String x:Key="LOC_GI_MultipleGamesRemoved">L'integrazione GlosSI/Steam di {0} giochi è stata rimossa.</sys:String>

